package test1;

public class LinkedList {
	private Node head;
	private Node tail;
	private int length;

	public LinkedList(int value) {
		Node newNode = new Node(value);
		head = newNode;
		tail = newNode;
		length = 1;
	}
	
	public void printList() {
		Node temp =head;
		while (temp != null) {
			System.out.println(temp.value);
			temp =temp.next;
		}
	}

	public void getHead() {
		
	}
	
	public void getTail() {
		
	}
	public void getLength() {
		
	}
	
}
