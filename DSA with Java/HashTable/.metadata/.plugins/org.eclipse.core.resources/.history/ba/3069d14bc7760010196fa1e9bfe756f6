package datastructure.hashtable;

public class HashTable {
int size = 7;

private Node[] dataMap;

public HashTable(int value) {
dataMap = new Node(size);
}

  class Node {
	  String key;
	  int value;
	  Node next;
	  
	  Node(String key, int value){
		  this.key = key;
		  this.value = value;
	  }
	  
  }
	
}
