package tree;

public class BinarySearchTree {
   Node root;
	
//  public BinarySearchTree(int value) {
//	  Node newNode = new Node(value);
//	  root = newNode;
//  }
	
   public boolean insert(int value) {
	   Node newNode= new Node(value);
	   if(root == null) {
		   root = newNode;
		   return true;
	   }
	   Node temp = root;
	   
	   while(true) {
		   if(newNode.value == temp.value) {
			   return false;
		   }
		   
	   }
	   
	   
	   
   }
   
	class Node{
		Node left;
		Node right;
		int value;
		
		Node(int value){
			this.value = value;
		}
	}
	
}
